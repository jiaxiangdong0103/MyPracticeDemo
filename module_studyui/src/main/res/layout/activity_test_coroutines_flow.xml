<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/sy_dp10">

    <TextView
        android:id="@+id/tvHelloSimple"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sy_dp50"
        android:layout_marginTop="@dimen/sy_dp10"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="sequence"
        android:textColor="@color/ptb_ffffff"
        android:textSize="@dimen/sy_text_dp20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvHelloFlow"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sy_dp50"
        android:layout_marginTop="@dimen/sy_dp10"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="Flow"
        android:textColor="@color/ptb_ffffff"
        android:textSize="@dimen/sy_text_dp20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvHelloSimple" />
    <TextView
        android:id="@+id/tvHelloAsFlow"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sy_dp50"
        android:layout_marginTop="@dimen/sy_dp10"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="AsFlow"
        android:textColor="@color/ptb_ffffff"
        android:textSize="@dimen/sy_text_dp20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvHelloFlow" />
    <TextView
        android:id="@+id/tvHelloReduce"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sy_dp50"
        android:layout_marginTop="@dimen/sy_dp10"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="Reduce求和"
        android:textColor="@color/ptb_ffffff"
        android:textSize="@dimen/sy_text_dp20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvHelloAsFlow" />
    <TextView
        android:id="@+id/tvHelloFlowOn"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sy_dp50"
        android:layout_marginTop="@dimen/sy_dp10"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="helloContext"
        android:textColor="@color/ptb_ffffff"
        android:textSize="@dimen/sy_text_dp20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvHelloReduce" />

</androidx.constraintlayout.widget.ConstraintLayout>